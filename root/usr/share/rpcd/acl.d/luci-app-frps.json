{
	"luci-app-frps": {
		"description": "Grant access to LuCI app frps",
		"read": {
			"file": {
				"/etc/group": [ "read" ],
				"/etc/passwd": [ "read" ],
				"/etc/init.d/frps *": [ "exec" ],
				"/etc/init.d/frps start": [ "exec" ],
				"/etc/init.d/frps stop": [ "exec" ],
				"/etc/init.d/frps restart": [ "exec" ],
				"/etc/init.d/frps reload": [ "exec" ],
				"/etc/init.d/frps enable": [ "exec" ],
				"/etc/init.d/frps disable": [ "exec" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": ["frps"]
		},
		"write": {
			"uci": ["frps"]
		}
	}
}

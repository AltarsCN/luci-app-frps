{
	"luci-app-frps": {
		"description": "Grant access to LuCI app frps",
		"read": {
			"file": {
				"/etc/group": [ "read" ],
				"/etc/passwd": [ "read" ]
			},
			"ubus": {
				"service": [ "list" ],
				"rc": [ "list" ]
			},
			"uci": ["frps"]
		},
		"write": {
			"ubus": {
				"rc": [ "init" ]
			},
			"uci": ["frps"]
		}
	}
}

